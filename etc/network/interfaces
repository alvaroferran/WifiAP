#auto wlan0
#allow-hotplug wlan0
#iface wlan0 inet dhcp
#wpa-conf /etc/wpa_supplicant/wpa_supplicant.conf
#iface default inet dhcp


#CREATE ACCESS POINT
auto wlan0
iface wlan0 inet static
  address 192.168.42.1
  netmask 255.255.255.0


#Local loopback
auto lo
iface lo inet loopback
